{
  "name": "@ucfopen/equation-sandbox",
  "materia": {
    "cleanName": "equation-sandbox"
  },
  "version": "1.0.1",
  "dependencies": {
    "angular": "~1.5.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "materia-widget-development-kit": "^2.2.0",
    "onchange": "^4.0.0",
    "pegjs": "^0.8.0",
    "webpack-synchronizable-shell-plugin": "^0.0.7",
    "mathquill": "0.9.1",
    "jsxgraph": "^0.99.7"
  },
  "devDependencies": {
    "sass-graph": "^2.1.1"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "build-dev": "webpack --verbose",
    "test": "karma start ./karma.conf.js --no-auto-watch --single-run",
    "test-ci": "karma start ./karma.conf.js --no-auto-watch --single-run",
    "pretest": "npm run build-dev",
    "pretestFull": "npm run build-dev",
    "testFull": "karma start ./karma-full.conf.js",
    "peg:build": "pegjs --cache ./src/peg/latex.pegjs ./src/js/latex-parser.js",
    "peg:watch": "onchange './src/peg/latex.pegjs' -- yarn peg:build"
  },
  "author": "University of Central Florida, Center for Distributed Learning",
  "description": "Template widget to use as an example.",
  "license": "AGPL-3.0"
}
